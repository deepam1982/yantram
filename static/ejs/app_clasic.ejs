		}

		.powerOffIcon {
			position:absolute;
			right: -20px;
			top: -21px;
			height:24px; width:24px;
			padding:10px;
			}
		.powerOffIcon .bgDiv{
			height:24px; width:24px;
			line-height:24px;
			border-radius: 12px;
			font-size: 18px;
			text-align: center;
		}
		.patchIcon {
			position:absolute;
			right: -20px;
			height:24px; width:24px;
			padding:10px;
		}
		.patchIcon .bgDiv{
			height:24px; width:24px;
			line-height:24px;
			font-size: 18px;
			text-align: center;
			border:1px solid white;
			border-radius: 15px;
		}
		.dialogCont{padding:20px;}
		.dialog{padding:20px; position:relative;}
		.dialog .msgDiv {text-align: left;}
		.dialog .buttonDiv {margin-top: 10px;}
