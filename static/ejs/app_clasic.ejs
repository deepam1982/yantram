		.multiSelectIconTemplate {width:45px; height:60px; float:left;position:relative;}
		.editSensorRulePannel .multiSelectIconTemplate { margin:5px;}
		.crossIcon {
			width:20px; height:20px;
			background-image: url("/static/<$=revId$>/images/transparentCross.png");
			background-size: cover; 
			position:absolute; right:0px; top:13px; z-index: 1;
			border-radius: 10px;
			filter: progid:DXImageTransform.Microsoft.AlphaImageLoader( src='/static/<$=revId$>/images/transparentCross.png', sizingMethod='scale');
		    -ms-filter: "progid:DXImageTransform.Microsoft.AlphaImageLoader( src='/static/<$=revId$>/images/transparentCross.png', sizingMethod='scale')";

		}
		.tick {
			width:20px; height:20px;
			background-image: url("/static/<$=revId$>/images/transparentTick.png");
			background-size: cover; 
			position:absolute; right:0px; top:13px; z-index: 1;
			border-radius: 10px;
			filter: progid:DXImageTransform.Microsoft.AlphaImageLoader( src='/static/<$=revId$>/images/transparentTick.png', sizingMethod='scale');
		    -ms-filter: "progid:DXImageTransform.Microsoft.AlphaImageLoader( src='/static/<$=revId$>/images/transparentTick.png', sizingMethod='scale')";

		}
