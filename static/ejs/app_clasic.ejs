				<% for(var i=0; i<ipArr.length; i++) {%>
					<span <% if(i+1 == ipArr.length)print("style='display:none;' id='dummyClstrStng'");%> class='clstrStng'>
						</br></br>
						<span style='margin:8px 0 0 -160px;float:left;'>Cluster <%print(i+2)%> Static IP Address:</span><input type='text' placeholder='<$=ipMask$>.125' style='width:86px;' value='<%=localIpArr[i+1]%>' orig-value='<%=localIpArr[i+1]%>'></input><span class='remove' style='float:right;margin:10px 10px 0 0;'><i class='fa fa-trash-o font14'></i></span>
					</span>
				<% } %>	
				</br></br>
